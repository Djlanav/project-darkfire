[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust-src/target/debug/librust_src.so"
linux.release.x86_64 =   "res://rust-src/target/release/librust_src.so"
windows.debug.x86_64 =   "res://rust-src/target/debug/rust_src.dll"
windows.release.x86_64 = "res://rust-src/target/release/rust_src.dll"
macos.debug =            "res://rust-src/target/debug/librust_src.dylib"
macos.release =          "res://rust-src/target/release/librust_src.dylib"
macos.debug.arm64 =      "res://rust-src/target/debug/librust_src.dylib"
macos.release.arm64 =    "res://rust-src/target/release/librust_src.dylib"

[icons]
RustPlayer = "res://icons/RustPlayer3D.svg"
